<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".EventBusBActivity">

    <TextView
        android:id="@+id/tv_sticky_event"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="this is for recive sticky event" />

    <TextView
        android:id="@+id/tv_sticky_back"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="this is for send back event" />

    <Button
        android:id="@+id/btn_close"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Eventbus handle Sticky back event" />

    <Button
        android:id="@+id/btn_close2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Sticky back boolean" />

</LinearLayout>